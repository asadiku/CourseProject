{
    "name": "CS 410 Coursera BM25",
    "description": "Provides additional search functionality for coursera",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
		"tabs",
		"background",
        "identity",
        "notifications"
	],
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "http://127.0.0.1:8000/",
        "http://127.0.0.1:5000/"
    ]
}